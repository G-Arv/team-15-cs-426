{
    "compilerOptions": {
      "target": "ES2020",
      "module": "nodenext",
      "moduleResolution": "nodenext",
      "lib": ["ES2020"],
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "rootDir": "src",
      "outDir": "dist"
    },
    "exclude": ["node_modules", "logs"]
  }
  